journal = {Scientific Reports},
number = {July 2015},
pages = {1--9},
pmid = {26884149},
publisher = {Nature Publishing Group},
title = {{The roles and impacts of human hunter-gatherers in North Pacific marine food webs}},
url = {http://dx.doi.org/10.1038/srep21179},
volume = {6},
year = {2016}
}
