number = {1},
pages = {1--6},
title = {{Seasonality of stable carbon and nitrogen isotopes within the pelagic food web of Taihu Lake}},
url = {http://journals.cambridge.org/action/displayAbstract?fromPage=online{\&}aid=8146403},
volume = {44},
year = {2008}
}
