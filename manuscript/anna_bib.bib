issn = {00063606},
journal = {Biotropica},
keywords = {community structure,food-webs,pitcher plant,tropical},
number = {2},
pages = {218},
title = {{Structure and Dynamics in Nepenthes madagascariensis Pitcher Plant Micro-Communities}},
